<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
    <persistence-unit name="eav-crosscheck-pu" transaction-type="JTA">
        <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
        <jta-data-source>jdbc/RepPool</jta-data-source>
        <class>com.bsbnb.creditregistry.portlets.crosscheck.dm.Message</class>
        <class>com.bsbnb.creditregistry.portlets.crosscheck.dm.CrossCheckMessage</class>
        <class>com.bsbnb.creditregistry.portlets.crosscheck.dm.PortalUser</class>
        <class>com.bsbnb.creditregistry.portlets.crosscheck.dm.CrossCheck</class>
        <class>com.bsbnb.creditregistry.portlets.crosscheck.dm.SubjectType</class>
        <class>com.bsbnb.creditregistry.portlets.crosscheck.dm.Creditor</class>
        <class>com.bsbnb.creditregistry.portlets.crosscheck.dm.Shared</class>
        <class>com.bsbnb.creditregistry.portlets.crosscheck.dm.Report</class>
        <exclude-unlisted-classes>false</exclude-unlisted-classes>
        <properties>
            <property name="eclipselink.cache.shared.default" value="false"/>
            <property name="eclipselink.logging.level" value="FINEST"/>
        </properties>
    </persistence-unit>
</persistence>
