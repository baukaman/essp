<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
  <persistence-unit name="report-portlet2PU" transaction-type="JTA">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <jta-data-source>jdbc/RepPool</jta-data-source>
    <class>kz.bsbnb.usci.portlet.report.dm.ReportInputParameter</class>
    <class>kz.bsbnb.usci.portlet.report.dm.Report</class>
    <class>kz.bsbnb.usci.portlet.report.dm.ExportType</class>
    <class>kz.bsbnb.usci.portlet.report.dm.ReportLoad</class>
    <class>kz.bsbnb.usci.portlet.report.dm.ReportLoadFile</class>
    <properties>
      <property name="eclipselink.cache.shared.default" value="false"/>
    </properties>
  </persistence-unit>
</persistence>
