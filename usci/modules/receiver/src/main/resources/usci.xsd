<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified"
            attributeFormDefault="unqualified">

    <xsd:element name="batch" type="batch"/>
    <xsd:element name="entities" type="entities"/>

    <xsd:complexType name="batch">
        <xsd:all>
            <xsd:element name="entities" type="entities"/>
        </xsd:all>
    </xsd:complexType>

    <xsd:simpleType name="operation">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="DELETE"/>
            <xsd:enumeration value="CLOSE"/>
            <xsd:enumeration value="INSERT"/>
            <xsd:enumeration value="NEW"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:complexType name="entities">
        <xsd:choice minOccurs="1" maxOccurs="unbounded">
            <xsd:element name="credit">
                <xsd:complexType>
                    <xsd:complexContent>
                        <xsd:extension base="credit">
                            <xsd:attribute name="operation" type="operation" use="optional"/>
                        </xsd:extension>
                    </xsd:complexContent>
                </xsd:complexType>
            </xsd:element>
        </xsd:choice>
    </xsd:complexType>

    <xsd:complexType name="person_name">
        <xsd:all>
            <xsd:element name="middlename" type="xsd:string" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="lastname" type="xsd:string" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="firstname" type="xsd:string" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="lang" type="xsd:string" minOccurs="0" maxOccurs="1" nillable="true"/>
        </xsd:all>
    </xsd:complexType>

    <xsd:complexType name="remains">
        <xsd:all>
            <xsd:element name="limit" type="remains_limit" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="discounted_value" type="remains_discounted_value" minOccurs="0" maxOccurs="1"
                         nillable="true"/>
            <xsd:element name="debt" type="remains_debt" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="interest" type="remains_interest" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="correction" type="remains_correction" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="discount" type="remains_discount" minOccurs="0" maxOccurs="1" nillable="true"/>
        </xsd:all>
    </xsd:complexType>

    <xsd:complexType name="change">
        <xsd:all>
            <xsd:element name="remains" type="remains" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="turnover" type="turnover" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="credit_flow" type="credit_flow" minOccurs="0" maxOccurs="1" nillable="true"/>
        </xsd:all>
    </xsd:complexType>

    <xsd:complexType name="remains_interest">
        <xsd:all>
            <xsd:element name="pastdue" type="remains_interest_pastdue" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="current" type="remains_interest_current" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="write_off" type="remains_interest_write_off" minOccurs="0" maxOccurs="1"
                         nillable="true"/>
        </xsd:all>
    </xsd:complexType>

    <xsd:complexType name="credit_flow">
        <xsd:all>
            <xsd:element name="classification" type="ref_classification" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="provision" type="provision_group" minOccurs="0" maxOccurs="1" nillable="true"/>
        </xsd:all>
    </xsd:complexType>

    <xsd:complexType name="ref_econ_trade">
        <xsd:all>
            <xsd:element name="name_ru" type="xsd:string" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="code" type="xsd:string" minOccurs="1" maxOccurs="1" nillable="false"/>
            <xsd:element name="name_kz" type="xsd:string" minOccurs="0" maxOccurs="1" nillable="true"/>
        </xsd:all>
    </xsd:complexType>

    <xsd:complexType name="ref_classification">
        <xsd:all>
            <xsd:element name="provision_portfolio_max" type="xsd:double" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="provision_debt" type="xsd:double" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="name_ru" type="xsd:string" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="comment_ru" type="xsd:string" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="provision_portfolio_min" type="xsd:double" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="is_input_value" type="xsd:boolean" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="code" type="xsd:string" minOccurs="1" maxOccurs="1" nillable="false"/>
            <xsd:element name="value_" type="xsd:double" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="comment_kz" type="xsd:string" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="name_kz" type="xsd:string" minOccurs="0" maxOccurs="1" nillable="true"/>
        </xsd:all>
    </xsd:complexType>

    <xsd:complexType name="turnover">
        <xsd:all>
            <xsd:element name="issue" type="turnover_issue" minOccurs="0" maxOccurs="1" nillable="true"/>
        </xsd:all>
    </xsd:complexType>

    <xsd:complexType name="ref_bank_relation">
        <xsd:all>
            <xsd:element name="name_ru" type="xsd:string" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="code" type="xsd:string" minOccurs="1" maxOccurs="1" nillable="false"/>
            <xsd:element name="name_kz" type="xsd:string" minOccurs="0" maxOccurs="1" nillable="true"/>
        </xsd:all>
    </xsd:complexType>

    <xsd:complexType name="bank_relation">
        <xsd:all>
            <xsd:element name="bank_relation" type="ref_bank_relation" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="creditor" type="ref_creditor" minOccurs="0" maxOccurs="1" nillable="true"/>
        </xsd:all>
    </xsd:complexType>

    <xsd:complexType name="credit">
        <xsd:all>
            <xsd:element name="finance_source" type="ref_finance_source" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="interest_rate_yearly" type="xsd:double" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="pledges" type="pledge_set" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="contract" type="contract" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="change" type="change" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="subject" type="subject" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="portfolio" type="portfolio" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="actual_issue_date" type="date" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="contract_maturity_date" type="date" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="currency" type="ref_currency" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="amount" type="xsd:double" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="credit_object" type="ref_credit_object" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="maturity_date" type="date" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="credit_purpose" type="ref_credit_purpose" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="creditor_branch" type="ref_creditor_branch" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="credit_type" type="ref_credit_type" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="prolongation_date" type="date" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="creditor" type="ref_creditor" minOccurs="1" maxOccurs="1" nillable="false"/>
            <xsd:element name="has_currency_earn" type="xsd:boolean" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="primary_contract" type="primary_contract" minOccurs="1" maxOccurs="1" nillable="false"/>
        </xsd:all>
    </xsd:complexType>

    <xsd:complexType name="head">
        <xsd:all>
            <xsd:element name="names" type="person_name_set" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="docs" type="document_set" minOccurs="0" maxOccurs="1" nillable="true"/>
        </xsd:all>
    </xsd:complexType>

    <xsd:complexType name="portfolio_flow_kfn">
        <xsd:all>
            <xsd:element name="details" type="portfolio_flow_detail_set" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="portfolio" type="ref_portfolio" minOccurs="1" maxOccurs="1" nillable="false"/>
        </xsd:all>
    </xsd:complexType>

    <xsd:complexType name="remains_interest_current">
        <xsd:all>
            <xsd:element name="value_currency" type="xsd:double" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="balance_account" type="ref_balance_account" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="value" type="xsd:double" minOccurs="0" maxOccurs="1" nillable="true"/>
        </xsd:all>
    </xsd:complexType>

    <xsd:complexType name="ref_ba_drt">
        <xsd:all>
            <xsd:element name="balance_account" type="ref_balance_account" minOccurs="1" maxOccurs="1"
                         nillable="false"/>
            <xsd:element name="debt_remains_type" type="ref_debt_remains_type" minOccurs="1" maxOccurs="1"
                         nillable="false"/>
        </xsd:all>
    </xsd:complexType>

    <xsd:complexType name="contract">
        <xsd:all>
            <xsd:element name="no" type="xsd:string" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="date" type="date" minOccurs="0" maxOccurs="1" nillable="true"/>
        </xsd:all>
    </xsd:complexType>

    <xsd:complexType name="portfolio">
        <xsd:all>
            <xsd:element name="portfolio_msfo" type="ref_portfolio" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="portfolio" type="ref_portfolio" minOccurs="0" maxOccurs="1" nillable="true"/>
        </xsd:all>
    </xsd:complexType>

    <xsd:complexType name="remains_limit">
        <xsd:all>
            <xsd:element name="value_currency" type="xsd:double" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="balance_account" type="ref_balance_account" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="value" type="xsd:double" minOccurs="0" maxOccurs="1" nillable="true"/>
        </xsd:all>
    </xsd:complexType>

    <xsd:complexType name="ref_enterprise_type">
        <xsd:all>
            <xsd:element name="is_organization" type="xsd:boolean" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="name_ru" type="xsd:string" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="is_se" type="xsd:boolean" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="code" type="xsd:string" minOccurs="1" maxOccurs="1" nillable="false"/>
            <xsd:element name="name_kz" type="xsd:string" minOccurs="0" maxOccurs="1" nillable="true"/>
        </xsd:all>
    </xsd:complexType>

    <xsd:complexType name="portfolio_data">
        <xsd:all>
            <xsd:element name="portfolio_flows_kfn" type="portfolio_flow_kfn_set" minOccurs="0" maxOccurs="1"
                         nillable="true"/>
            <xsd:element name="portfolio_flows_msfo" type="portfolio_flow_msfo_set" minOccurs="0" maxOccurs="1"
                         nillable="true"/>
            <xsd:element name="creditor" type="ref_creditor" minOccurs="1" maxOccurs="1" nillable="false"/>
        </xsd:all>
    </xsd:complexType>

    <xsd:complexType name="ref_portfolio">
        <xsd:all>
            <xsd:element name="name_ru" type="xsd:string" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="code" type="xsd:string" minOccurs="1" maxOccurs="1" nillable="false"/>
            <xsd:element name="creditor" type="ref_creditor" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="name_kz" type="xsd:string" minOccurs="0" maxOccurs="1" nillable="true"/>
        </xsd:all>
    </xsd:complexType>

    <xsd:complexType name="ref_doc_type">
        <xsd:all>
            <xsd:element name="weight" type="xsd:int" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="name_ru" type="xsd:string" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="is_organization_doc" type="xsd:boolean" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="code" type="xsd:string" minOccurs="1" maxOccurs="1" nillable="false"/>
            <xsd:element name="short_name_kz" type="xsd:string" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="is_identification" type="xsd:boolean" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="short_name_ru" type="xsd:string" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="name_kz" type="xsd:string" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="is_person_doc" type="xsd:boolean" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="sign_count" type="xsd:int" minOccurs="0" maxOccurs="1" nillable="true"/>
        </xsd:all>
    </xsd:complexType>

    <xsd:complexType name="ref_offshore">
        <xsd:all>
            <xsd:element name="name_ru" type="xsd:string" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="code" type="xsd:string" minOccurs="1" maxOccurs="1" nillable="false"/>
            <xsd:element name="name_kz" type="xsd:string" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="country" type="ref_country" minOccurs="0" maxOccurs="1" nillable="true"/>
        </xsd:all>
    </xsd:complexType>

    <xsd:complexType name="ref_credit_purpose">
        <xsd:all>
            <xsd:element name="name_ru" type="xsd:string" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="code" type="xsd:string" minOccurs="1" maxOccurs="1" nillable="false"/>
            <xsd:element name="name_kz" type="xsd:string" minOccurs="0" maxOccurs="1" nillable="true"/>
        </xsd:all>
    </xsd:complexType>

    <xsd:complexType name="ref_debtor_type">
        <xsd:all>
            <xsd:element name="name_ru" type="xsd:string" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="code" type="xsd:string" minOccurs="1" maxOccurs="1" nillable="false"/>
            <xsd:element name="name_kz" type="xsd:string" minOccurs="0" maxOccurs="1" nillable="true"/>
        </xsd:all>
    </xsd:complexType>

    <xsd:complexType name="remains_debt_write_off">
        <xsd:all>
            <xsd:element name="value_currency" type="xsd:double" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="balance_account" type="ref_balance_account" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="value" type="xsd:double" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="date" type="date" minOccurs="0" maxOccurs="1" nillable="true"/>
        </xsd:all>
    </xsd:complexType>

    <xsd:complexType name="pledge">
        <xsd:all>
            <xsd:element name="contract" type="xsd:string" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="value" type="xsd:double" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="pledge_type" type="ref_pledge_type" minOccurs="0" maxOccurs="1" nillable="true"/>
        </xsd:all>
    </xsd:complexType>

    <xsd:complexType name="ref_balance_account">
        <xsd:all>
            <xsd:element name="sixth_sign" type="xsd:string" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="is_in_balance" type="xsd:boolean" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="display_no" type="xsd:string" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="name_ru" type="xsd:string" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="seventh_sign" type="xsd:string" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="first_fourth_signs" type="xsd:string" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="no_" type="xsd:string" minOccurs="1" maxOccurs="1" nillable="false"/>
            <xsd:element name="fifth_sign" type="xsd:string" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="name_kz" type="xsd:string" minOccurs="0" maxOccurs="1" nillable="true"/>
        </xsd:all>
    </xsd:complexType>

    <xsd:complexType name="person_info">
        <xsd:all>
            <xsd:element name="offshore" type="ref_offshore" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="names" type="person_name_set" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="addresses" type="address_set" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="bank_relations" type="bank_relation_set" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="contacts" type="contact_set" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="country" type="ref_country" minOccurs="0" maxOccurs="1" nillable="true"/>
        </xsd:all>
    </xsd:complexType>

    <xsd:complexType name="ref_econ_sector">
        <xsd:all>
            <xsd:element name="name_ru" type="xsd:string" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="code" type="xsd:string" minOccurs="1" maxOccurs="1" nillable="false"/>
            <xsd:element name="name_kz" type="xsd:string" minOccurs="0" maxOccurs="1" nillable="true"/>
        </xsd:all>
    </xsd:complexType>

    <xsd:complexType name="ref_currency">
        <xsd:all>
            <xsd:element name="name_ru" type="xsd:string" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="rating" type="xsd:string" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="code" type="xsd:string" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="short_name" type="xsd:string" minOccurs="1" maxOccurs="1" nillable="false"/>
            <xsd:element name="name_kz" type="xsd:string" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="is_convertible" type="xsd:boolean" minOccurs="0" maxOccurs="1" nillable="true"/>
        </xsd:all>
    </xsd:complexType>

    <xsd:complexType name="ref_finance_source">
        <xsd:all>
            <xsd:element name="name_ru" type="xsd:string" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="code" type="xsd:string" minOccurs="1" maxOccurs="1" nillable="false"/>
            <xsd:element name="name_kz" type="xsd:string" minOccurs="0" maxOccurs="1" nillable="true"/>
        </xsd:all>
    </xsd:complexType>

    <xsd:complexType name="organization_name">
        <xsd:all>
            <xsd:element name="name" type="xsd:string" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="lang" type="xsd:string" minOccurs="0" maxOccurs="1" nillable="true"/>
        </xsd:all>
    </xsd:complexType>

    <xsd:complexType name="provision_group">
        <xsd:all>
            <xsd:element name="provision_msfo" type="provision" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="provision_kfn" type="provision" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="provision_msfo_over_balance" type="provision" minOccurs="0" maxOccurs="1"
                         nillable="true"/>
        </xsd:all>
    </xsd:complexType>

    <xsd:complexType name="ref_exclusive_doc">
        <xsd:all>
            <xsd:element name="doc_type" type="ref_doc_type" minOccurs="1" maxOccurs="1" nillable="false"/>
            <xsd:element name="code" type="xsd:string" minOccurs="1" maxOccurs="1" nillable="false"/>
        </xsd:all>
    </xsd:complexType>

    <xsd:complexType name="remains_interest_write_off">
        <xsd:all>
            <xsd:element name="value_currency" type="xsd:double" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="value" type="xsd:double" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="date" type="date" minOccurs="0" maxOccurs="1" nillable="true"/>
        </xsd:all>
    </xsd:complexType>

    <xsd:complexType name="ref_region">
        <xsd:all>
            <xsd:element name="name_ru" type="xsd:string" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="code" type="xsd:string" minOccurs="1" maxOccurs="1" nillable="false"/>
            <xsd:element name="name_kz" type="xsd:string" minOccurs="0" maxOccurs="1" nillable="true"/>
        </xsd:all>
    </xsd:complexType>

    <xsd:complexType name="ref_credit_type">
        <xsd:all>
            <xsd:element name="name_ru" type="xsd:string" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="kind_id" type="xsd:int" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="code" type="xsd:string" minOccurs="1" maxOccurs="1" nillable="false"/>
            <xsd:element name="name_kz" type="xsd:string" minOccurs="0" maxOccurs="1" nillable="true"/>
        </xsd:all>
    </xsd:complexType>

    <xsd:complexType name="remains_discount">
        <xsd:all>
            <xsd:element name="value_currency" type="xsd:double" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="balance_account" type="ref_balance_account" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="value" type="xsd:double" minOccurs="0" maxOccurs="1" nillable="true"/>
        </xsd:all>
    </xsd:complexType>

    <xsd:complexType name="subject">
        <xsd:all>
            <xsd:element name="organization_info" type="organization_info" minOccurs="0" maxOccurs="1" nillable="true"/>
            <xsd:element name="is_organization" minOccurs="1" maxOccurs="1" nillable="false">
                <xsd:complexType>
                    <xsd:simpleContent>
                        <xsd:extension base="xsd:boolean">
                            <xsd:attribute type="operation" name="operation" use="optional"/>
                            <xsd:attribute type="xsd:boolean" name="data" use="optional"/>
                        </xsd:extension>
                    </xsd:simpleContent>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="docs" type="document_set" minOccurs="1" maxOccurs="1" nillable="false"/>
            <xsd:element name="is_person" minOccurs="1" maxOccurs="1" nillable="false">
                <xsd:complexType>
                    <xsd:simpleContent>
                        <xsd:extension base="xsd:boolean">
